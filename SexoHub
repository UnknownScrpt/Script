local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SexoHub", "Midnight")
local Tab = Window:NewTab("Admin")
local Section = Tab:NewSection("Tenha cuidado")
Section:NewKeybind("ToggleUI", "KeybindInfo", Enum.KeyCode.E, function()
	Library:ToggleUI()
end)
Section:NewButton("InfYield", "ButtonInfo", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
Section:NewButton("Blackhub", "ButtonInfo", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FastyGame31/black-hub/main/fuck%20you"))()
end)
local Tab = Window:NewTab("Farm")
local Section = Tab:NewSection("Treinamento")
Section:NewToggle("FastPsy 1M", "ToggleInfo", function(state)
    if state then
        local args = {
            [1] = {
                [1] = "Skill_Invisible",
                [2] = "Start"
            }
        }
        
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        wait(1)
        plr = game.Players.LocalPlayer
        hum = plr.Character.HumanoidRootPart
        hum.CFrame = CFrame.new(-2542, 5487, -551) -- coloca a coordenada do lugar q vc quer teleportar aq
        local Player = game.Players.LocalPlayer
        getgenv().AutoPsychic = true

while AutoPsychic == true do
    for FastPP = 1,9 do 
        wait()
local args = {[1] = {[1] = "+PP3"}}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
end
    else
        getgenv().AutoPsychic = false

while AutoPsychic == true do
    for FastPP = 1,9 do 
        wait()
local args = {[1] = {[1] = "+PP3"}}
game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
end
    end
end)
